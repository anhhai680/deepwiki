[flake8]
max-line-length = 88
max-complexity = 10
exclude = 
    .git,
    __pycache__,
    .venv,
    .venv/*,
    venv,
    venv/*,
    .pytest_cache,
    .next,
    node_modules,
    backend/__pycache__,
    test/__pycache__
ignore = 
    E203,
    W503,
    E501
per-file-ignores =
    __init__.py:F401
